{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/topics",
      "dest": "/api/topics.js"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/api/users.js"
    },
    {
      "src": "/api/execute",
      "dest": "/api/execute.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
